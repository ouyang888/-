@import "../../style/weui.wxss";
.homecs {
  margin-top: 10rpx;
  background: rgb(247, 247, 247);
}

.csTop {
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
}

.csTop text {
  font-size: 32rpx;
}

.csTop navigator {
  font-size: 24rpx;
}
.grey{
  background: #cccccc !important;
}


/* .csList {
  padding: 23rpx;
} */

.csCont {
  padding-bottom: 20rpx;
  background: #fff;
  padding: 30rpx;
  margin-bottom: 20rpx;
}

.csCont image {
  width: 100%;
  height: 350rpx;
}

.csCont text {
  font-size: 28rpx;
}

.cssDateBtn {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}

.cssDateBtn text {
  font-size: 26rpx;
  display: table;
  color: #a0a0a0;
  margin-top: 14rpx;
}

.btn {
  font-size: 28rpx;
  margin: 0;
  background: #ffa200;
  color: #fff;
  width: 190rpx;
}

.end {
  background: rgb(203, 203, 203);
  font-size: 28rpx;
  margin: 0;
  color: #fff;
  border: none;
  width: 190rpx;
}

.search {
  width: 100%;
}

.search_arr {
  border: 1px solid #d0d0d0;
  border-radius: 50rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
}

.search_arr input {
  margin-left: 66rpx;
  height: 70rpx;
  border-radius: 5px;
}

.bc_text {
  line-height: 68rpx;
  height: 68rpx;
  margin-top: 34rpx;
}

.sousuo {
  margin-left: 30rpx;
  width: 15%;
  line-height: 150%;
  text-align: center;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  color: rgb(102, 102, 102);
}

.page_row {
  display: flex;
  flex-direction: row;
  margin-bottom: 10rpx;
  justify-content: space-between
}

.searchcion {
  margin: 20rpx 10rpx 10rpx 10rpx;
  position: absolute;
  left: 25rpx;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}

/* 弹框 */

.show-btn {
  margin-top: 100rpx;
  color: #2c2;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 90%;
  overflow: hidden;
  position: fixed;
  top: 27%;
  left: 5%;
  z-index: 9999;
  background: #f9f9f9;
  border-radius: 10rpx;
  height: auto;
}

.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-content {
  padding: 50rpx 32rpx;
}

.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  font-size: 34rpx;
  line-height: 86rpx;
  padding-bottom: 58rpx;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 90%;
  color: #fff;
  text-align: center;
  background: rgb(204, 204, 204);
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 54rpx;
}

.inputBox {
  width: 90%;
  margin: 0 auto;
  margin-top: 60rpx;
}

.inputBox ul li {
  font-size: 30rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f3f3f3;
  padding: 20rpx 10rpx;
  justify-content: start;
}

.inputBox ul li text {
  width: 145rpx;
  font-size: 28rpx;
}

.inputBox ul li input {
  font-size: 28rpx;
}

.code {
  height: 67rpx;
  font-size: 24rpx;
  border: 1px solid #ffebd1;
  color: #fbb75b;
  display: flex;
  align-items: center;
  justify-content: center;
}

.btnView {
  width: 150rpx;
  white-space: nowrap;
}

.codeInputs {
  margin-left: 10rpx;
}
.showNothing{
  font-size: 15px;
  color: #8D8D8D;
  text-align: center;
  margin-top: 20px;
}