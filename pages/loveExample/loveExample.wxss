/* pages/loveExample/loveExample.wxss */
page {
  background:rgb(247,247,247);
}

.loveHead{
  position: relative;
  padding:55rpx 0;
}

.vipBg{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 600rpx;
}

.vipBg image{
  width: 100%;
  height: 100%;
}

.vipjy{
  position: relative;
  z-index: 1;
  text-align: center;
}

.vipjy image{
  width: 230rpx;
  height: 190rpx;
}

.vipJyz{
  font-size:30rpx;
  color:#fff;
  margin-top:20rpx;
}

.vipLevel{
  position: relative;
  padding:40rpx 60rpx;
}

.vipTxt{
  color:#fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size:26rpx;
  text-align: center;
}

.vipLevel progress{
  margin:20rpx 0;
}

.levFen{
  position: relative;
  color:#9C9EA9;
  text-align: center;
  font-size:24rpx;
}

.loveCont{
  background: #fff;
  margin-top:20rpx;
  padding:30rpx 30rpx;
}

.vipTit{
  font-size: 32rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.vipTit::before{
  content: '';
  background: #FEA432;
  display: inline-block;
  width: 10rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.loveCont text{
  font-size:30rpx;
  color:#888888;
}

.vipLevTable{
  border-radius:22rpx;
  background:#FFF9E1;
  overflow: hidden;
  margin-top:30rpx;
}

.tablehead{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background:#FEA432;
}

.tablehead view{
  width: 33.3%;
  color:#fff;
  text-align: center;
  font-size:32rpx;
  padding:20rpx 0;
  border-right: 1px solid #E3E1DD;
}

.tablecont{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.tablecont view{
  width: 33.3%;
  color:#67656A;
  text-align: center;
  font-size:32rpx;
  padding:20rpx 0;
  border-right: 1px solid #E3E1DD;
  border-bottom: 1px solid #E3E1DD;
}

.tablecont view:last-child{
  border-right:none;
}

.vipLevTable view.tablecont:last-child view{
  border-bottom: none;
}

.tableCont2 view:first-child{
  width: 70%;
}

.tableCont2 view:last-child{
  width: 30%;
}