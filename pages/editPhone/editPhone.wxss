.loginBtn {
  background: #ccc;
  height: 80rpx;
  width: 690rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  font-size: 32rpx;
  margin: 0 auto;
  margin-top: 60rpx;
  color: #fff;
  letter-spacing: 5rpx;
}

.loginBtn input {
  text-align: center;
  width: 100%;
  height: 38rpx;
}

.inputBox {
  width: 100%;
  margin: 0 auto;
  padding:0 34rpx;
  box-sizing: border-box
}

.inputBox ul li {
  font-size: 30rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f3f3f3;
  padding: 40rpx 0rpx;
  justify-content: space-between;
}

.inputBox ul li text {
  width: 185rpx;
}

/* .inputBox ul li input {
  margin-left: 30rpx;
} */

.code {
  height: 64rpx;
  font-size: 24rpx;
  border: 1px solid #ffebd1;
  color: #fbb75b;
  display: flex;
  align-items: center;
  justify-content: center;
  background:#fff;
  width: 150rpx;
}

.btnView {
  width: 150rpx;
  white-space: nowrap;
}
.subMit{
  background: rgb(255, 165, 47);
  font-size: 32rpx;
}
.bangding{
  font-size: 34rpx;
  color: rgb(102, 102, 102);
  text-align: right
}
